    "validate:types": "echo 'ðŸ” Checking TypeScript types...' && tsc --noEmit",
    "validate:lint": "echo 'ðŸ” Checking code quality...' && next lint --max-warnings 0",
    "validate:format": "echo 'ðŸ” Checking code formatting...' && prettier --check .",
    "validate:policies": "echo 'ðŸ” Checking policy compliance...' && node scripts/validate-file.js --all",
    "validate:file": "node scripts/validate-file.js",
    "fix": "npm run lint:fix && npm run format",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "ts-node prisma/seed.ts",
    "db:studio": "prisma studio",
    "prepare": "husky"
